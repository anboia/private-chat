# Environment
ENVIRONMENT=development
DEBUG=false

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_API_BASE=https://api.openai.com/v1

# Client API Keys (comma-separated)
CLIENT_API_KEYS=client-key-1,client-key-2,client-key-3

# API Key Mapping (JSON format mapping client keys to OpenAI keys)
# API_KEY_MAPPING={"client-key-1": "sk-openai-key-1", "client-key-2": "sk-openai-key-2"}

# Rate Limiting
RATE_LIMIT__REQUESTS_PER_MINUTE=60
RATE_LIMIT__TOKENS_PER_MINUTE=100000

# Cache Configuration
CACHE__REDIS_URL=redis://localhost:6379
CACHE__EMBEDDINGS_TTL=3600
CACHE__DEFAULT_TTL=300

# Retry Configuration
RETRY_MAX_ATTEMPTS=3
RETRY_BACKOFF_FACTOR=2.0

# Logging
LOG_LEVEL=INFO